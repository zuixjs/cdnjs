{
  "name": "pouchdb",
  "npmName": "pouchdb",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "pouchdb.js",
        "pouchdb.min.js"
      ]
    }
  ],
  "version": "4.0.1",
  "filename": "pouchdb.min.js",
  "description": "PouchDB is a pocket-sized database.",
  "homepage": "http://pouchdb.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/pouchdb/pouchdb"
  },
  "keywords": [
    "db",
    "couchdb",
    "pouchdb"
  ],
  "tags": [
    "db",
    "couchdb",
    "pouchdb"
  ],
  "dependencies": {
    "argsarray": "0.0.1",
    "bluebird": "~1.0.0",
    "inherits": "~2.0.1",
    "level-js": "^2.1.3",
    "level-sublevel": "~5.2.0",
    "leveldown": "~0.10.2",
    "levelup": "~0.18.2",
    "lie": "^2.6.0",
    "localstorage-down": "~0.4.4",
    "pouchdb-mapreduce": "~2.2.1",
    "request": "~2.28.0",
    "extend": "^1.2.1",
    "md5-jkmyers": "0.0.1",
    "through2": "^0.4.1",
    "es3ify": "^0.1.3"
  },
  "browser": {
    "./deps/buffer": false,
    "request": false,
    "crypto": false,
    "./adapters/leveldb": false,
    "./adapters/levelalt": false,
    "./lib/deps/ajax.js": "./lib/deps/ajax-browser.js",
    "./lib/version.js": "./lib/.version.js",
    "bluebird": "lie",
    "leveldown": "level-js"
  },
  "browserify": {
    "transform": [
      "./node_modules/es3ify"
    ]
  }
}
