{
  "name": "soundplayer-widget",
  "version": "0.4.1",
  "description": "Lightweight embedded SoundCloud player widgets",
  "style": "cssnext/main.css",
  "directories": {
    "example": "example"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "stage": 0
        }
      ],
      "envify"
    ]
  },
  "keywords": [
    "soundcloud",
    "widget",
    "player",
    "embed",
    "react",
    "deku",
    "functional",
    "component"
  ],
  "author": "Dmitri Voronianski <dmitri.voronianski@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "deku": "^0.4.2",
    "soundcloud-audio": "^0.1.4"
  },
  "npmName": "soundplayer-widget",
  "npmFileMap": [
    {
      "basePath": "/dist",
      "files": [
        "**/*"
      ]
    }
  ],
  "filename": "soundplayer-widget.min.js"
}
