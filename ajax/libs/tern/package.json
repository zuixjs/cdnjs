{
  "name": "tern",
  "filename": "tern.min.js",
  "license": "MIT",
  "version": "0.20.0",
  "author": "Marijn Haverbeke <marijnh@gmail.com>",
  "description": "A JavaScript code analyzer for deep, cross-editor language support",
  "repository": {
    "type": "git",
    "url": "git://github.com/ternjs/tern.git"
  },
  "dependencies": {
    "acorn": "^3.2.0",
    "enhanced-resolve": "^2.2.2",
    "glob": "3",
    "minimatch": "0.2",
    "resolve-from": "2.0.0"
  },
  "keywords": [
    "analyzer",
    "cross-editor",
    "javascript"
  ],
  "homepage": "https://ternjs.net/",
  "autoupdate": {
    "target": "tern",
    "fileMap": [
      {
        "basePath": "lib",
        "files": [
          "*.+(js|map)"
        ]
      },
      {
        "basePath": "",
        "files": [
          "plugin/*.+(js|map)"
        ]
      }
    ],
    "source": "npm"
  }
}
